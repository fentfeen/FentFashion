local Players = game:GetService('Players');
local RunService = game:GetService('RunService');

local function CharacterAddedCrown(Character)
    local display = Character:WaitForChild('Humanoid').DisplayName
    Character:WaitForChild('Humanoid').DisplayName = "[ðŸ‘‘]"..display.."";
end;

Players.PlayerAdded:Connect(function(Player)
    Player.CharacterAdded:Wait();
    if Player.Name == "droolingforassrnxd" then
        Player.CharacterAdded:Connect(CharacterAddedCrown);
        CharacterAddedCrown(Player.Character);
    end
end);

Players.PlayerAdded:Connect(function(Player)
    Player.CharacterAdded:Wait();
    if Player.UserId == "3499991340" then
        Player.CharacterAdded:Connect(CharacterAddedCrown);
        CharacterAddedCrown(Player.Character);
    end
end);

Players.PlayerAdded:Connect(function(Player)
    Player.CharacterAdded:Wait();
    if Player.Name == "lwcv" then
        Player.CharacterAdded:Connect(CharacterAddedCrown);
        CharacterAddedCrown(Player.Character);
    end
end);

